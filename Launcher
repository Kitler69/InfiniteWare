local SupportedGames = {
    Slap_Battle = {6403373529};
    Fisch = {16732694052};
    Baseplate = {4483381587};
 }

    if table.find(SupportedGames.Slap_Battle,game.PlaceId) then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Kitler69/InfiniteWare/refs/heads/main/SlapBattle.lua",true))()
       elseif table.find(SupportedGames.Fisch,game.PlaceId) then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Kitler69/InfiniteWare/refs/heads/main/Fisch",true))()
        elseif table.find(SupportedGames.Baseplate,game.PlaceId) then
        print("WORKS WOOO")
    end
